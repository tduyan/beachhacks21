{
    "name" : "BeachHacks21",
    "description": "Just for Fun!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Grind"
    }
}